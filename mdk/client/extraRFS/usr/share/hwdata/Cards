LINE     #Option "pci_retry"    # faster, but problematic for ISA DMA
LINE     #Option "hw_cursor"    # Use hardware cursor (see docs for limitations)
LINE     #Option "xaa_no_color_exp"  # When text (or bitmap) is not rendered correctly

